%head
  %meta{:charset => "utf-8"}
  %meta{:name => "viewport", :content => "width=device-width, initial-scale=1, maximum-scale=1"}
  %title= 'Tracker'
  = stylesheet_link_tag 'application', media: :all
  = stylesheet_link_tag "http://code.jquery.com/ui/#{Tracker::Application.jquery_ui_version}/themes/base/jquery-ui.css"
  %script{src: "//ajax.googleapis.com/ajax/libs/jquery/#{Tracker::Application.jquery_version}/jquery.min.js"}
  %script{src: "//ajax.googleapis.com/ajax/libs/jqueryui/#{Tracker::Application.jquery_ui_version}/jquery-ui.min.js"}
  / = stylesheet_link_tag 'jquery-ui/1.10.0/jquery-ui.css'
  / = javascript_include_tag 'jquery.min'
  / = javascript_include_tag 'jquery-ui'
  = javascript_include_tag 'application'
  = csrf_meta_tags
  = yield :head
