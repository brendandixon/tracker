%tr
  %td= feature.title
  %td= feature.service.abbreviation
  %td= feature.release_date.in_time_zone.to_date.to_s(:mm_dd_yy) if feature.release_date.present?
  %td
    %a{href: feature.contact_us_link}= feature.contact_us_number
  %td= project_labels(feature)
  %td
    = render partial: 'shared/links', locals: { links: [:edit, :destroy], object: feature}
